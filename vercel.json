{
  "version": 2,
  "buildCommand": "CI=false npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "create-react-app",
  "outputDirectory": "build",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "nodeVersion": "22.x",
        "installCommand": "npm install --legacy-peer-deps",
        "buildCommand": "CI=false npm run build"
      }
    }
  ]
} 